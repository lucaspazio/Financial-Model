{
  "name": "SpArks Financial Simulator",
  "forwardPorts": [5000],
  "postCreateCommand": "pip install -r requirements.txt",
  "customizations": {
    "codespaces": {
      "openFiles": ["main.py"]
    }
  },
  "containerEnv": {
    "FLASK_APP": "main.py",
    "FLASK_ENV": "development"
  },
  "portsAttributes": {
    "5000": {
      "label": "SpArks Simulator",
      "onAutoForward": "openPreview"
    }
  }
}
